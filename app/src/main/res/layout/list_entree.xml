<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"

    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent" android:layout_height="match_parent">

    <TextView
        android:id="@+id/listeTextViewLib"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/listeTextViewCode"
        android:layout_alignBottom="@+id/listeTextViewCode"
        android:layout_marginLeft="25dp"
        android:layout_marginBottom="0dp"
        android:layout_toRightOf="@+id/listeTextViewCode"
        android:text="TextView" />

    <TextView
        android:id="@+id/listeTextViewStock"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/listeTextViewLib"
        android:layout_alignBottom="@+id/listeTextViewLib"
        android:layout_marginLeft="14dp"
        android:layout_marginRight="32dp"
        android:layout_marginBottom="0dp"
        android:layout_toLeftOf="@+id/qte"
        android:layout_toRightOf="@+id/listeTextViewLib"
        android:text="TextView"
        tools:ignore="UnknownId" />

    <TextView
        android:id="@+id/listeTextViewCode"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="28dp"
        android:text="TextView" />

</RelativeLayout>